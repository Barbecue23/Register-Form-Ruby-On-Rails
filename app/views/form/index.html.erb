<div class="form-container">
<h1>All User</h1>

<table>
    <head>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Gender</th>
        <th>Email</th>
        <th>Phone</th>
        <th>subject</th>
        <th>delete</th>
    </head>

    <tbody>
    <% @forms.each do |form| %>
        <div>
            <td><%= form.firstname %></td>
            <td><%= form.lastname %></td>
            <td><%= form.gender %></td>
            <td><%= form.email %></td>
            <td><%= form.phonenumber %></td>
            <td><%= form.subject %></td>
            <td>
            <%= button_to 'ลบ', "form/#{form.id}", method: :delete%>
            </td>
        </div>
        </tr>
    <% end %>
    </tbody>
    </table>
    <div class="botton_allList">
    <%= link_to 'กลับหน้าลงทะเบียน', "form/new" %>
    </div>
</div>